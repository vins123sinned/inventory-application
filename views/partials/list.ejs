<main class="list">
  <h1 class="list-heading"><%= title %></h1>
  <section class="list-grid">
    <% list.forEach((item) => { %>
      <a href="<%= link %><%= item.id %>" class="list-link">
        <div class="list-cell">
          <img src="<%= item.image_link %>" width="300" height="300">
          <p><%= item.name %></p>
        </div>
      </a>
    <% }); %>
    <a href="<%= link %>create" class="list-link">
      <div class="list-cell create-card">
        <img src="/images/cards/add-card.jpg" width="300" alt="A medley of fruits">
        <p><%= addText %></p>
      </div>
    </a>
  </section>
</main>